{
    "name": "t3s/content-consent",
    "type": "typo3-cms-extension",
    "description": "Provides a content consent plugin to load any content elements and custom plugins by ajax without jQuery! So you can include Google Maps or YouTube videos GDPR/DSGVO compliant. Best used with Bootstrap 5",
    "authors": [
        {
            "name": "Helmut Hackbarth",
            "role": "Developer"
        }
    ],
    "license": "GPL-2.0-or-later",
    "require": {
        "typo3/cms-core": "^12.4"
    },
    "autoload": {
        "psr-4": {
            "T3S\\ContentConsent\\": "Classes"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "T3S\\ContentConsent\\Tests\\": "Tests"
        }
    },
    "replace": {
        "typo3-ter/content-consent": "self.version"
    },
    "config": {
        "vendor-dir": ".Build/vendor",
        "bin-dir": ".Build/bin"
    },
    "extra": {
        "typo3/cms": {
            "web-dir": ".Build/public",
            "extension-key": "content_consent"
        }
    }
}